#ifndef __STDLIB__
#define __STDLIB__

extern "C" {
  #include <malloc.h>
  #include <types.h>
}

void *operator new(unsigned int size)
{
    return malloc(size);
}

void *operator new[](unsigned int size)
{
    return malloc(size);
}

void operator delete(void *p)
{
    free(p);
}

void operator delete[](void *p)
{
    free(p);
}

#endif // __STDLIB__
